{% extends '::base.front.html.twig' %}
{% block title %}
    {% if data.page %}
        {{ data.page.getHtmlTitle }}
    {% else %}
        404 - Page not found
    {% endif %}
{% endblock %}

{% block meta %}
     {% if data.page %}
        <meta name="keywords" content="{{data.page.getMetaKeywords}}"/>
        <meta name="description" content="{{data.page.getMetaDescription}}"/>
    {% endif %}
{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{asset('bundles/aliansstone/css/front.style.css')}}" type="text/css" media="screen"/>  
{% endblock %}

{% block content %}
    <div class="wrap">
   {% if data.page %}
        <h1>{{data.page.getTitle}}</h1>
        {{data.page.getContent | raw}}
   {% else %}
        {% include 'AliansStoneBundle:Front:404.html.twig' %}
   {% endif %}
    </div>
{% endblock %}


{% block customjs %}
<script type="text/javascript">
    
</script>
{% endblock %}
