        <![endif]-->
    
    	<!-- CSS Implementing Plugins -->
        <link rel="stylesheet" href="{{asset('bundles/aliansstone/css/jquery-ui.css')}}" type="text/css" media="screen"/>
        <link rel="stylesheet" href="{{asset('bundles/aliansstone/js/fancybox/jquery.fancybox.css')}}" type="text/css" media="screen"/>
