    <body>
        {% block header %}
            {% include "AliansStoneBundle:Front:_header.html.twig" %}
        {% endblock %}
        {% block slider %}{% endblock %}
        
        {% block content %}{% endblock %}
        
        {% block menu %}{% endblock %}
        {% block materials %}{% endblock %}
        
        {% block epigraph %}{% endblock %}
        {% block projects %}{% endblock %}
        
        {% block partners %}
            {% include "AliansStoneBundle:Front:_partners.html.twig" %}
        {% endblock %}
        
        {% block topfooter %}
            {% include "AliansStoneBundle:Front:_topfooter.html.twig" %}
        {% endblock %}
        {% block bottomfooter %}
            {% include "AliansStoneBundle:Front:_bottomfooter.html.twig" %}
        {% endblock %}
        
        {% block javascripts %}
            <!-- JS Global Compulsory -->			
            
            <script type="text/javascript" src="{{asset('bundles/aliansstone/assets/custom/js/modernizr.custom.js')}}"></script>		
            <script type="text/javascript" src="{{asset('bundles/aliansstone/assets/bootstrap/js/bootstrap.min.js')}}"></script>	
             <script type="text/javascript" src="{{asset('bundles/aliansstone/magnific-popup/jquery.magnific-popup.min.js')}}"></script>	
