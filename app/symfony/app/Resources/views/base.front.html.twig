        <link href="{{asset('bundles/aliansstone/assets/custom/css/business-plate.css')}}" rel="stylesheet"/>
        <link rel="shortcut icon" href="{{asset('bundles/aliansstone/assets/custom/ico/favicon.ico')}}"/>
        <meta name="wot-verification" content="9e622856cd581eb89040"/> 
        {% block stylesheets %}{% endblock %}
