            <script type="text/javascript" src="{{asset('bundles/aliansstone/assets/custom/js/index.js')}}"></script>
            
            
            <script type="text/javascript">
               jQuery(document).ready(function() {
                    $('#topcontrol').css({'position': 'fixed', 'bottom': '20px', 'right': '20px', 'cursor': 'pointer', 'opacity': '1'});
                   
            
            /**fancybox**/
            
             $("#single_1").fancybox({
              helpers: {
                  title : {
                      type : 'float'
                  }
              }
          });
          
          });
